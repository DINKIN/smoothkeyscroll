{
  "name": "Smooth Key Scroll",
  "version": "0.2",
  "description": "Smooth keyboard scrolling for Google Chrome",
  "offline_enabled": true,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"]
    }
  ],
  "permissions": [
     "storage"
   ],
  "options_page": "options/options.html",
  "manifest_version": 2
}
